{
    "monitor_q":{
        "db_engine": "{{ db_engine }}",
        "db_options": {
            "db_name": "{{ db_name }}",
            "db_user": "{{ db_user }}",
            "db_passwd": "{{ db_passwd }}",
            "echo": {{ db_echo|default(false) }}
        },   
        "logging":{
            "enable": true,
            "log_level": "{{ log_level }}"
        }
    },
    "rabbitMQ": {
        "queues": [
            {
                "name": "monitor_q",
                "bindings": [
                    {
                        "exchange": "{{ rabbit_exchange_name }}",
                        "routing_key": "*.monitor-get.*"
                    },
                    {
                        "exchange": "{{ rabbit_exchange_name }}",
                        "routing_key": "*.monitor-put.*"
                    }
                ]
            }
        ]
    }
}